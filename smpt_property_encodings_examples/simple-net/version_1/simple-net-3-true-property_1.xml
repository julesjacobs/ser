<?xml version='1.0' encoding='utf-8'?>
<property-set>
  <property>
    <id>DisjunctionExample</id>
    <description>(2P1 + P2 â‰¥ 4) OR (P1 = P2)</description>
    <formula>
      <exists-path>
        <finally>
          <disjunction>
            <integer-ge>
              <integer-add>
                <integer-mul>
                  <integer-constant>2</integer-constant>
                  <tokens-count><place>P1</place></tokens-count>
                </integer-mul>
                <tokens-count><place>P2</place></tokens-count>
              </integer-add>
              <integer-constant>4</integer-constant>
            </integer-ge>
            <integer-eq>
              <tokens-count><place>P1</place></tokens-count>
              <tokens-count><place>P2</place></tokens-count>
            </integer-eq>
          </disjunction>
        </finally>
      </exists-path>
    </formula>
  </property>
</property-set>
